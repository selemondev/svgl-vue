{
  "name": "@selemondev/svgl-vue",
  "description": "An optimized package with SVG logos to be used as Vue components âœ¨",
  "private": false,
  "version": "0.0.3",
  "packageManager": "pnpm@8.15.4",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "svgl",
    "svgl-vue",
    "logos",
    "svg",
    "vue",
    "nuxt"
  ],
  "scripts": {
    "build": "tsup --onSuccess \"pnpm build:dts\"",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "generate:components": "pnpm dlx esno scripts/index.ts && prettier --write ./src/**/*.vue",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "generate:release": "pnpm dlx changelogen@latest --release && npm publish",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.48",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^9.17.0",
    "fs-extra": "^11.3.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "~5.3.0",
    "unplugin-vue": "^6.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^1.8.25"
  }
}
