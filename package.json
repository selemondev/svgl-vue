{
	"name": "@selemondev/svgl-vue",
	"description": "An optimized package with SVG logos to be used as Vue components âœ¨",
	"private": false,
	"version": "0.0.3",
	"packageManager": "pnpm@8.15.9",
	"type": "module",
	"files": [
		"dist"
	],
	"keywords": [
		"svgl",
		"svgl-vue",
		"logos",
		"svg",
		"vue",
		"nuxt"
	],
	"scripts": {
		"build:package": "tsdown",
		"dev:build": "tsdown --watch ./src/index.ts",
		"dev": "tsx ./src/index.ts",
		"play:dev": "pnpm -C playground dev",
		"play:build": "pnpm -C playground build",
		"biome": "biome check --write",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"generate:components": "pnpm dlx esno scripts/index.ts && prettier --write ./src/**/*.vue",
		"generate:release": "pnpx changelogen@latest --release && git push --follow-tags",
		"generate:major:release": "pnpx changelogen@latest --major && git push --follow-tags",
		"generate:minor:release": "pnpx changelogen@latest --minor && git push --follow-tags",
		"generate:patch:release": "pnpx changelogen@latest --patch && git push --follow-tags",
		"bump:version": "pnpx changelogen@latest --bump",
		"package:beta": "pnpm build:package && pnpm publish --tag beta",
		"package": "pnpm build:package && pnpm publish --access=public"
	},
	"peerDependencies": {
		"vue": "^3.5.21"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.12.0",
		"@tsconfig/node20": "^20.1.6",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^24.3.1",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.8.1",
		"eslint": "^8.49.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-vue": "^9.33.0",
		"fs-extra": "^11.3.1",
		"prettier": "^3.6.2",
		"tsdown": "^0.14.2",
		"typescript": "^5.9.2",
		"unplugin-vue": "^6.2.0",
		"vue-tsc": "^3.0.6"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	}
}
