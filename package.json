{
  "name": "@selemondev/svgl-vue",
  "description": "An optimized package with SVG logos to be used as Vue components âœ¨",
  "private": false,
  "version": "2.0.4",
  "packageManager": "pnpm@8.15.9",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selemondev/svgl-vue.git"
  },
  "keywords": [
    "svgl",
    "svgl-vue",
    "logos",
    "svg",
    "vue",
    "nuxt"
  ],
  "sideEffects": false,
  "scripts": {
    "build:package": "tsdown",
    "dev:build": "tsdown --watch ./src/index.ts",
    "dev": "tsx ./src/index.ts",
    "biome": "biome check --write",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "generate:components": "pnpm dlx esno scripts/index.ts && pnpm biome",
    "generate:release": "pnpx changelogen@latest --release --hide-author-email && git push --follow-tags",
    "generate:major:release": "pnpx changelogen@latest --major && git push --follow-tags",
    "generate:minor:release": "pnpx changelogen@latest --minor && git push --follow-tags",
    "generate:patch:release": "pnpx changelogen@latest --patch && git push --follow-tags",
    "bump:version": "pnpx changelogen@latest --bump",
    "package:beta": "pnpm build:package && pnpm publish --tag beta",
    "package": "pnpm build:package && pnpm publish --access=public"
  },
  "peerDependencies": {
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@tsconfig/node20": "^20.1.9",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.13",
    "@vue/tsconfig": "^0.8.1",
    "fs-extra": "^11.3.3",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "unplugin-vue": "^6.2.0",
    "vue-tsc": "^3.2.4"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index-Dnq0l9GR.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  }
}
